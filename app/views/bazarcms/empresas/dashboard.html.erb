<b>Empresas</b>
<br/>
<table class="tabla">
<tr>
	<td>
		<table cellspacing="0">
		<tr>
			<th colspan="2">
				Ultimas Empresas
			</th>
		</tr>	
		<% for emp in @ultimas %>
		<tr>
			<td>
				<%= link_to emp.nombre, bazarcms_empresa_path(emp.id) %>
			</td>
			<td> 
				<%= link_to image_tag("lupa.png"), bazarcms_empresa_path(emp.id) %>
				<%= link_to image_tag("email.png"), new_mensaje_url+"?aquien=#{emp.id}&tipo=M" %>
			</td>
		</tr>
		<% end %>
		</table cellspacing="0">
	</td>
	<td>
		<table cellspacing="0">
		<tr>
			<th colspan="2">
				Ultimas Actualizadas
			</th>
		</tr>	
		<% for emp in @actualizadas %>
		<tr>
			<td>
				<%= link_to emp.nombre, bazarcms_empresa_path(emp.id) %>
			</td>
			<td>
				<%= link_to image_tag("lupa.png"), bazarcms_empresa_path(emp.id) %>
				<%= link_to image_tag("email.png"), new_mensaje_url+"?aquien=#{emp.id}&tipo=M" %>
			</td>
		</tr>
		<% end %>
		</table>
	
	</td>	
</tr>	
</table>
Total: <%= @total %>
