<b>Consultas</b>

<table class="tabla">
  <tr>
    <th>Desc</th>
    <th>Consultados</th>
    <th>Resultados</th>
    <th>SQL</th>
    <th>Lanzada</th>
    <th colspan="3">Acciones</th>
  </tr>

<% @empresasconsultas.each do |empresasconsulta| %>
  <tr>
  	<td><%= empresasconsulta.desc %></td>
  	<td><%= empresasconsulta.total_consultas %>/<%= empresasconsulta.total_respuestas %></td>
  	<td><%= empresasconsulta.total_resultados %></td>
  	<td><%= empresasconsulta.sql %></td>
  	<td><%= empresasconsulta.fecha_inicio.strftime("%Y-%m-%d %H:%M") -%>-<%= empresasconsulta.fecha_fin.strftime("%H:%M") %></td>
  	<td><%= link_to image_tag('lupa.png'), empresasconsulta %></td>
    <td><%= link_to image_tag('delete.png'), empresasconsulta, :confirm => 'Seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>
<br />
<%= will_paginate @empresasconsultas %>
<br />

<%= link_to 'Buscar Empresas', '/bazarcms/buscadorempresas' %> | 
<%= link_to 'Borrar consultas realizadas', '/bazarcms/borrarconsultas', :confirm => 'Seguro?' %>