

<% if @empresa.id == rating.ori_empresa_id && rating.ori_bazar_id == BZ_param('BazarId').to_i && !rating.des_fecha.nil? %>

<table class="tabla">
<tr>
	<td>
		<b>Empresa</b>
	</td>
	<td>
		<b>Fecha</b>
	</td>
</tr>
	
<tr>
	
	<td>
		<%= link_to(rating.des_empresa_nombre, bazarcms_empresa_path(rating.des_empresa_id, :bazar_id => rating.des_bazar_id)) %>
	</td>
	
	<td>
		<%= rating.des_fecha.strftime ("%d-%m-%Y") %>
	</td>
	
</tr>

<% if rating.role == 'C' %>

	<tr>
		<td><b>Expectativas</b></td>
		<td>
			<%= helper_rating_show(rating.des_proveedor_expectativas, "/bazarcms/ficharating/#{rating.ori_empresa_id}?bazar_id=#{rating.ori_bazar_id}").html_safe %>
		</td>
	</tr>

	<tr>
		<td><b>Plazos</b></td>
		<td>
			<%= helper_rating_show(rating.des_proveedor_plazos, "").html_safe %>
		</td>
	</tr>

	<tr>
		<td><b>Comunicaci贸n</b></td>
		<td>
			<%= helper_rating_show(rating.des_proveedor_comunicacion, "").html_safe %>
		</td>

	</tr>

<% else %>

	<tr>
		<td><b>Plazos</b></td>
		<td>
			<%= helper_rating_show(rating.ori_cliente_plazos, "").html_safe %>
		
		</td>
	</tr>
	<tr>
		<td><b>Comunicaci贸n</b></td>
		<td>
			<%= helper_rating_show(rating.ori_cliente_comunicacion, "").html_safe %>
		</td>

	</tr>

<% end %>

<tr>
	<td colspan="2">
		<b>Comentario:</b> <%= rating.ori_texto %>
	</td>
</tr>

</table>


<% end %>

